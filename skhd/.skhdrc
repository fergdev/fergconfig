ctrl + cmd - h : yabai -m window west --resize right:-20:0 2> /dev/null || yabai -m window --resize right:-20:0
ctrl + cmd - j : yabai -m window north --resize bottom:0:20 2> /dev/null || yabai -m window --resize bottom:0:20
ctrl + cmd - k : yabai -m window south --resize top:0:-20 2> /dev/null || yabai -m window --resize top:0:-20
ctrl + cmd - l : yabai -m window east --resize left:20:0 2> /dev/null || yabai -m window --resize left:20:0

alt - e : yabai -m window --toggle split

# shift + cmd - l : yabai -m window --warp east || yabai -m window --warp right
# shift + cmd - h : yabai -m window --warp west || yabai -m window --warp left
#
# ctrl + alt + cmd - m : yabai -m window --grid 1:1:0:0:1:1

### Swap/move window
#   (options: north, east, south, west)
alt + shift - h : yabai -m window --swap west  || $(yabai -m window --display west; yabai -m display --focus west)
alt + shift - j : yabai -m window --swap south || $(yabai -m window --display south; yabai -m display --focus south)
alt + shift - k : yabai -m window --swap north || $(yabai -m window --display north; yabai -m display --focus north)
alt + shift - l : yabai -m window --swap east  || $(yabai -m window --display east; yabai -m display --focus east)

ctrl - h : yabai -m window --focus west || yabi -m display --focus west
ctrl - j : yabai -m window --focus south || yabi -m display --focus south
ctrl - k : yabai -m window --focus north || yabi -m display --focus north
ctrl - l : yabai -m window --focus east || yabi -m display --focus east

